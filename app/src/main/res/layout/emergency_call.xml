<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="bmental.corgis.protomental.SignInLaunch">
    >





    <fragment xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        android:id="@+id/topBar"
        android:name="bmental.corgis.protomental.TopBar"
       tools:layout="@layout/fragment_patient_view"
        android:layout_width="match_parent"
        android:layout_height="150dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true" />

    <fragment xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        android:id="@+id/bottomBar"
        android:name="bmental.corgis.protomental.BottomBar"
        tools:layout="@layout/fragment_bottom_bar"
        android:layout_width="match_parent"
        android:layout_height="150dp"

        android:layout_alignParentBottom="true"
        android:layout_alignParentStart="true" />

    <!-- Real content-->

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Contact 1:"
        android:id="@+id/Contact1"
        android:layout_marginTop="90dp"
        android:textSize="40dp"
        android:layout_below="@+id/topBar"
        android:layout_alignParentStart="true" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Call"
        android:id="@+id/button"
        android:layout_marginEnd="30dp"
        android:layout_alignBottom="@+id/info1"
        android:layout_alignEnd="@+id/topBar" />

    <TextView
        android:layout_width="250dp"
        android:layout_height="wrap_content"
        android:text="Lexington, AL, 888-238-2828"
        android:id="@+id/info1"
        android:textSize="30dp"
        android:layout_marginStart="31dp"
        android:layout_alignBottom="@+id/Contact1"
        android:layout_toEndOf="@+id/Contact1" />




    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Contact 2:"
        android:id="@+id/Contact2"
        android:layout_marginTop="90dp"
        android:textSize="40dp"
        android:layout_below="@+id/Contact1"
        android:layout_alignParentStart="true" />
    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Call"
        android:id="@+id/call2"
        android:layout_marginEnd="30dp"
        android:layout_alignBottom="@+id/info2"
        android:layout_alignEnd="@+id/topBar" />

    <TextView
        android:layout_width="250dp"
        android:layout_height="wrap_content"
        android:text="Norfolk, MN, 888-238-7777"
        android:id="@+id/info2"
        android:textSize="30dp"
        android:layout_marginStart="31dp"
        android:layout_alignBottom="@+id/Contact2"
        android:layout_toEndOf="@+id/Contact2" />




    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Contact 2:"
        android:id="@+id/Contact3"
        android:layout_marginTop="90dp"
        android:textSize="40dp"
        android:layout_below="@+id/Contact2"
        android:layout_alignParentStart="true" />
    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Call"
        android:id="@+id/call3"
        android:layout_marginEnd="30dp"
        android:layout_alignBottom="@+id/info3"
        android:layout_alignEnd="@+id/topBar" />

    <TextView
        android:layout_width="250dp"
        android:layout_height="wrap_content"
        android:text="RealChester, MN, 999-999-9299"
        android:id="@+id/info3"
        android:textSize="30dp"
        android:layout_marginStart="31dp"
        android:layout_alignBottom="@+id/Contact3"
        android:layout_toEndOf="@+id/Contact3" />




</RelativeLayout>